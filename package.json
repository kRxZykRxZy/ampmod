    "devDependencies": {
        "esbuild-loader": "^4.4.0",
        "optimize-css-assets-webpack-plugin": "^6.0.1",
        "terser-webpack-plugin": "^4.2.3",
        "webpack": "^4.47.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.3"
    },
    "dependencies": {
        "@ampmod/branding": "workspace:*",
        "@fontsource-variable/inter": "^5.2.8"
    }
